		"xmlhttprequest": "^1.8.0"
	},
	"devDependencies": {
		"ava": "^1.2.1",
		"xo": "^0.24.0"
	},
	"xo": {
		"space": false,
		"semicolon": false,
		"capitalized-comments": [
			"error",
			"always",
			{
				"block": {
