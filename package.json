{
	"name": "alfred-anki",
	"version": "1.3.1",
	"description": "anki card generator",
	"license": "MIT",
	"repository": "bikenik/alfred-anki",
	"author": {
		"name": "bikenik",
		"email": "bikeniks@gmail.com",
		"url": "https://bikenik.org"
	},
	"engines": {
		"node": ">=4"
	},
	"scripts": {
		"test": "xo && ava",
		"postinstall": "alfy-init",
		"preuninstall": "alfy-cleanup"
	},
	"files": [
		"index.js",
		"icon.png",
		"info.plist",
		"src",
		"icons",
		"update-config.js"
	],
	"keywords": [
		"alfred",
		"workflow",
		"alfy"
	],
	"dependencies": {
		"alfy": "0.9.0",
		"fs-extra": "^8.1.0",
		"handlebars": "^4.1.2",
		"highlight.js": "^10.1.2",
		"html-entities": "^1.2.1",
		"jsonfile": "^5.0.0",
		"markdown-it": "^9.0.1",
		"markdown-it-abbr": "^1.0.4",
		"markdown-it-emoji": "^1.4.0",
		"markdown-it-ins": "^2.0.0",
		"markdown-it-mark": "^2.0.0",
		"p-map": "^3.0.0",
		"path-exists": "^4.0.0",
		"promise": "^8.0.3",
		"run-applescript": "^3.2.0",
		"xmlhttprequest": "^1.8.0"
	},
	"devDependencies": {
		"ava": "^2.2.0",
		"xo": "^0.24.0"
	},
	"xo": {
		"space": false,
		"semicolon": false,
		"capitalized-comments": [
			"error",
			"always",
			{
				"block": {
					"ignorePattern": "blockignore"
				}
			}
		]
	}
}
